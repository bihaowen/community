<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="life.bhw.conmmunity.dao.UserDao">

    <insert id="insertUser" parameterType="life.bhw.conmmunity.entity.User">
        insert into user(account_id,name,token,gmt_create,gmt_modified)
        values(#{account_id},#{name},#{token},#{gmt_create},#{gmt_modified})
    </insert>

</mapper>
